.nr ;F 0	\"when ;X = 5, LT macros in use
.nr !V 0	\"salutation flag
.nr !Y 0	\"attention flag
.nr !6 0	\"reference string flag
.nr !8 0	\"used for checking in )W
.nr !9 0	\"confidential flag
.nr !M 0	\"subject line flag
