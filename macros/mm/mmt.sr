'''\"	.(1 formats business letter in Blocked style
'''\"	Blocked:  The date line, the complimentary closing, and
'''\"	the writer's identification all begin at center.  All other lines
'''\"	begin at the left margin.  This is the default style
.de (1
.nr ;F 1	\"set letter type flag to indicate Blocked
.rm (2 (3 (4	\"removes macros that are not needed
.nf		\"set up environment for diversions
.in (\\n(.lu/2u)\"change indent to center of page
.rs		\"restore spacing-turned off by )h
.sp 11		\"start return address on line 12
'\"If return address diversion contains something
'\"then return address is put out,
'\"else skip down to line 15.
.ie \\n(!R \{\
.]s
.sp \}
.el .sp 3	\"date should be on line 15
\\*(DT\"date
'\"if confidential macro has been used then spit out string.
.if \\n(!9 \{\
.	sp
.	in 0	\"confidential notation is on left margin
\\*(}l \}
'\"if reference macro has been used then spit out string.
.if \\n(!6 \{\
.	sp
.	in (\\n(.lu/2u)	\"reference notation is centered
\\*(}k \}
'\"if confidential or reference notation has been used space down 3
'\"else space down 5
.ie (\\n(!9:\\n(!6) .sp 2  
.el .sp 4
.in 0		\"inside address is always left alligned
.if \w@\\*(}6@ \\*(}6	\"name of addressee
.if \w@\\*(}7@ \\*(}7	\"title of addressee
.]u		\"inside address
'\"if attention flag on, output attention string
.if \\n(!Y \{\
.sp
\\*(}j \}
'\"if salutation flag on output salutation string
.if \\n(!V \{\
.	sp
\\*(}a \}
'\"if subject line flag on output subject line string
.if \\n(!M \{\
.	sp
SUBJECT:  \\*(}m \}
.rm WA WE IA IE LO
.fi  
..
'''\"	.(2 formats business letter in Semiblocked style
'''\"	Semiblocked:  Exactly like the blocked except first line of each
'''\"	paragraph is indented five spaces.
.de (2
.nr ;F 2	\"set letter type flag to indicate Semiblocked
.rm (1 (3 (4	\"removes macros that are not needed
.rs		\"restorespacing turned off by )h
.sp 11		\"start on line 12
.nf		\"set up environment for diversions
.in (\\n(.lu/2u)\"change indent to center of page
'\"If return address diversion contains something
'\"then return address is put out,
'\"else skip done a few more lines to line 15.
.ie \\n(!R .]s
.el .sp 3
\\*(DT\"date
'\"if confidential macro has been used then spit out string.
.if \\n(!9 \{\
.	in 0	\"confidential notation is on left margin
.	sp
\\*(}l \}
'\"if reference macro has been used then spit out string.
.if \\n(!6 \{\
.	sp
.	in (\\n(.lu/2u)\"reference notation is centered
\\*(}k \}
'\"if confidential or reference notation move down to third line below
'\"else to fifth line below date
.ie (\\n(!9:\\n(!6) .sp 2
.el .sp 4
.in 0\"inside address is always left alligned
.if \w@\\*(}6@ \\*(}6\"name of addressee
.if \w@\\*(}7@ \\*(}7\"title of addressee
.]u	\"inside address
'\"if attention flag on output attention string
.if \\n(!Y \{\
.	sp
\\*(}j \}
'\"if salutation flag on output salutation string
.if \\n(!V \{\
.	sp
\\*(}a \}
'\"if subject line flag on output subject line string
.if \\n(!M \{\
.	sp
.	ti 5
SUBJECT:  \\*(}m \}
.rm WA WE IA IE LO
.nr Pt 1	\"paragraphs will by default be indented
.fi   
